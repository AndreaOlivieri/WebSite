              %p= t(:For_my_internship_I_developed_a_desktop_application_to_a_robotic_platform_with_the_aim_of_analyzing)
/ Start Skills section
%section.skills.border-bottom.padding-block
  / start container
  .container
    / start row
    .row
      .col-xs-12.col-sm-12.col-lg-6
        %h3.title.title-skills= t(:Professional_skills)
        %label.progress-bar-label Java
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"90", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 90%
        %label.progress-bar-label HTML
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"90", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 90%
        %label.progress-bar-label CSS &amp; SCSS
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"90", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 90%
        %label.progress-bar-label Javascript
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"85", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 85%
        %label.progress-bar-label JQuery
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"80", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 80%
        %label.progress-bar-label Ruby on Rails
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"70", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 70%
        %label.progress-bar-label Git
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"70", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 70%
        %label.progress-bar-label Photoshop
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"60", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 60%
        %label.progress-bar-label AngularJS
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"50", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 50%
        %label.progress-bar-label Meteor
        .progress
          .progress-bar{ role: "progressbar", "aria-valuenow"=>"30", "aria-valuemin"=>"0", "aria-valuemax"=>"100" }
            %span 30%
      .col-xs-12.col-sm-12.col-lg-6
        %h3.title.title-skills= t(:Additional_skills)
        .circle-progress-block
          .circle-progress
            %input{ type: "text", class: "dial", value: "90", "data-color"=>"#00b6f9", "data-from"=>"0", "data-to"=>"99"}
          %label.circle-progress-label= t(:Software_Development)
        .circle-progress-block
          .circle-progress
            %input{ type: "text", class: "dial", value: "90", "data-color"=>"#00b6f9", "data-from"=>"0", "data-to"=>"99"}
          %label.circle-progress-label= t(:Problem_Solving)
        .circle-progress-block
          .circle-progress
            %input{ type: "text", class: "dial", value: "70", "data-color"=>"#00b6f9", "data-from"=>"0", "data-to"=>"99"}
          %label.circle-progress-label= t(:Team_Work)
        .circle-progress-block
          .circle-progress
            %input{ type: "text", class: "dial", value: "60", "data-color"=>"#00b6f9", "data-from"=>"0", "data-to"=>"99"}
          %label.circle-progress-label= t(:English)
/ Start Info section
%section.info.border-bottom.padding-block.text-center
  / start container
  .container
    / start row
    .row
      .col-xs-12.col-sm-12.col-lg-12
        %h3.title= t(:Hobbies_Interest_html)
    .row
      .col-xs-12.col-sm-12.col-lg-12
        .circle-block
          %span.icon.hover-animate
            %i.fa.fa-futbol-o
          %span.name.hover-animate= t(:Football)
        .circle-block
          %span.icon.hover-animate
            %i.fa.fa-headphones
          %span.name.hover-animate= t(:Music)
        .circle-block
          %span.icon.hover-animate
            %i.fa.fa-rocket
          %span.name.hover-animate= t(:Travelling)
        .circle-block
          %span.icon.hover-animate
            %i.fa.fa-car
          %span.name.hover-animate= t(:Cars)
        .circle-block
          %span.icon.hover-animate
            %i.fa.fa-gamepad
          %span.name.hover-animate= t(:Video_games)
    %p
      / <a href="contact.html" class="btn hover-animate">Hire me Now</a>
      = link_to t(:Download_Resume), "assets/#{t(:Andrea_Olivieri_Resume)}.pdf", download: t(:Andrea_Olivieri_Resume), class: "btn btn-color hover-animate"