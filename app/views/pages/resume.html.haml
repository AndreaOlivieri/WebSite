                  %td= t(:Jul)+" 2015 - "+t(:Feb)+" 2016"
                %tr
                  %td.font-weight-m= t(:Post)
                  %td= t(:Web_Developer)
              %p= t(:As_part_time_job_I_worked_for_a_real_estate_agency)
            .item
              %table
                %tr
                  %td.font-weight-m.width-td1= t(:Experience_Name)
                  %td InnovAction Lab - Fashape
                %tr
                  %td.font-weight-m= t(:Address)
                  %td= t(:Rome_Italy)
                %tr
                  %td.font-weight-m= t(:Period)
                  %td= t(:Feb)+" 2015 - "+t(:May)+" 2015"
                %tr
                  %td.font-weight-m= t(:Post)
                  %td= t(:Co_founder_and_CTO_at_Fashape)
              %p= t(:InnovAction_Lab_is_a_training_course)
            .item
              %table
                %tr
                  %td.font-weight-m.width-td1= t(:Company_Name)
