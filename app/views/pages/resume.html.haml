              %p= t(:For_over_a_year_I_have_collaborated_for_developing_an_enterprise_software)
            .item
              %table
                %tr
                  %td.font-weight-m.width-td1= t(:Company_Name)
                  %td I.S.T.C. - C.N.R.
                %tr
                  %td.font-weight-m= t(:Address)
                  %td= t(:Via_S_Martino_della_Battaglia_44_00185_Rome_Italy)
                %tr
                  %td.font-weight-m= t(:Period)
                  %td= t(:Apr)+" 2013 - "+t(:Jul)+" 2013"
                %tr
                  %td.font-weight-m= t(:Post)
