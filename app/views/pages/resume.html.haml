- provide(:title, t(:Resume))
- provide(:page_class, 'resume_page')
/ Start Head section
%header.head
  / start container
  .container
    / start row
    .row
      .col-xs-8.col-sm-11.col-lg-11
        = image_tag "resume_l.png", class:"logo-page", alt: t(:Resume)
        / Title Page
        %h2.title= t(:Resume)
      .col-xs-4.col-sm-1.col-lg-1.text-right
        = link_to "", root_path, class: "btn-close hover-animate"
/ Start Content section
%section.content.padding-block.border-bottom
  / start container
  .container
    / start row
    .row
      .col-xs-12.col-sm-12.col-lg-6
        %h3.title.title-resume= t(:Education)
        .block-grey
          #education-slider.owl-carousel.owl-theme
            .item
              %table
                %tr
                  %td.font-weight-m.width-td= t(:Name)
                  %td= t(:Roma_Tre_Computer_Engineer)
                %tr
                  %td.font-weight-m= t(:Address)
                  %td= t(:Via_Ostiense_159_00154_Rome_Italy)
                %tr
                  %td.font-weight-m= t(:Period)
                  %td= t(:Sep)+" 2013 - "+t(:Today)
                %tr
                  %td.font-weight-m= t(:Level)
                  %td 8 EQF
                %tr
              %p= t(:Studying_for_Master_s_Degree_in_Computer_Engineering)
            .item
              %table
                %tr
                  %td.font-weight-m.width-td= t(:Name)
                  %td= t(:Roma_Tre_Computer_Engineer)
                %tr
                  %td.font-weight-m= t(:Address)
                  %td= t(:Via_Ostiense_159_00154_Rome_Italy)
                %tr
                  %td.font-weight-m= t(:Period)
                  %td= t(:Sep)+" 2010 - "+t(:Jul)+" 2013"
                %tr
                  %td.font-weight-m= t(:Level)
                  %td 6 EQF
              %p= t(:Bachelor_s_Degree_in_Computer_Engineering)
            .item
              %table
                %tr
                  %td.font-weight-m.width-td= t(:Name)
                  %td I.T.I.S. G.Armellini
                %tr
                  %td.font-weight-m= t(:Address)
                  %td= t(:Largo_B_Placido_Riccardi_13_00146_Rome_Italy)
                %tr
                  %td.font-weight-m= t(:Period)
                  %td= t(:Sep)+" 2005 - "+t(:Jun)+" 2010"
                %tr
                  %td.font-weight-m= t(:Level)
                  %td 4 EQF
              %p= t(:Diploma_of_Computer_Science)
      .col-xs-12.col-sm-12.col-lg-6
        %h3.title.title-resume= t(:Professional_Experience)
        .block-grey
          #work-slider.owl-carousel.owl-theme
            .item
              %table
                %tr
                  %td.font-weight-m.width-td1= t(:Company_Name)
